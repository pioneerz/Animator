<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.administrator.animatortest">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <activity android:name=".viewanimatior.ViewAnimatorActivity"
            android:theme="@style/AppTheme">
            <intent-filter>
                <action android:name="com.example.administrator.animatortest.viewanimator"/>
                <category android:name="android.intent.category.DEFAULT"/>
            </intent-filter>
        </activity>

        <activity android:name=".propertyanimator.PropertyAnimatorActivity"
            android:theme="@style/AppTheme">
            <intent-filter>
                <action android:name="com.example.administrator.animatortest.propertyanimator.propertyanimatoractivity"/>
                <category android:name="android.intent.category.DEFAULT"/>
            </intent-filter>
        </activity>

        <activity android:name=".valueAnimator.ValueAnimActivity"
            android:theme="@style/AppTheme">
            <intent-filter>
                <action android:name="com.example.administrator.animatortest.valueAnimator.valueanimactivity"/>
                <category android:name="android.intent.category.DEFAULT"/>
            </intent-filter>
        </activity>
    </application>

</manifest>